---
apiVersion: v1
kind: Namespace
metadata:
  name: {{ namespace }}
  labels:
    app.kubernetes.io/part-of: dss
---
apiVersion: v1
kind: PersistentVolumeClaim
metadata:
  name: {{ notebook_pvc_name }}
  namespace: {{ namespace }}
spec:
  accessModes:
  - ReadWriteOnce
  resources:
    requests:
      storage: 10Gi
  volumeMode: Filesystem
